n        color: '#b5cea8'},\n    {tag: [t.meta],\n        color: '#d4d4d4'},\n    {tag: [t.comment],\n        color: '#6a9955'},\n    {tag: t.atom,\n        color: '#9cdcfe' },\n    {tag: t.string,\n        color: '#4ec9b0' },\n    {tag: t.namespace,\n        color: '#569cd6', fontWeight: 'bold' },\n    {tag: t.className,\n        color: '#c586c0' },\n    {tag: t.name,\n        color: '#e40', fontWeight: 'bold' },\n    {tag: t.propertyName,\n        color: '#e56735' },\n])\n\n/// Extension to enable the Mox theme (both the editor theme and\n/// the highlight style).\nexport const mainsailTheme: Extension = [mainsailEditor, mainsailHighlightStyle]"],"sourceRoot":""}]}